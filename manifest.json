{
  "manifest_version": 3,
  "name": "Leetcode Helper",
  "description": "Nice to have helper utilities for leetcode",
  "version": "0.0.1",
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://*.leetcode.com/*"]
    }
  ],
  "permissions": ["storage"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
