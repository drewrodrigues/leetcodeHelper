{
  "manifest_version": 3,
  "name": "Leetcode Helper",
  "description": "Nice to have helper utilities for leetcode",
  "version": "1.0",
  "content_scripts": [
    {
      "js": ["index.js"],
      "matches": ["https://*.leetcode.com/explore/*/card/*"]
    },
    {
      "js": ["difficultyHider.js"],
      "matches": ["https://*.leetcode.com/problems/*"]
    }
  ]
}
